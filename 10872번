N! 출력하기


import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int N = input.nextInt();
        int result = 1;
        for (int i=1; i<(N+1); i++) {
            result *= i;
        }
        System.out.println(result);
    }
}
